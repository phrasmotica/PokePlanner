<UserControl x:Class="PokePlanner.Controls.HMChart"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:controls="clr-namespace:PokePlanner.Controls"
             xmlns:util="clr-namespace:PokePlanner.Util"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">

    <Label Background="{Binding Source={x:Static util:MyBrushes.PastelGreen}}" BorderThickness="2" BorderBrush="White">
        <StackPanel x:Name="panel">
            <Label Content="Hidden machine coverage" FontWeight="Bold" />
        </StackPanel>
    </Label>

</UserControl>